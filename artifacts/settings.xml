<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
      <localRepository/>
      <interactiveMode/>
      <usePluginRegistry/>
      <offline/>
      <pluginGroups/>
<servers/>
<mirrors>
    <mirror>
      <!--This sends everything else to /public -->
      <id>nexus</id>
      <mirrorOf>*</mirrorOf>
      <url>http://nexus-demo-cicd.18.196.187.53.nip.io/nexus/content/groups/public/</url>
    </mirror>
  </mirrors>
      <proxies/>
      <profiles>
        <profile>
          <id>fuse-profile</id>
            <repositories>
              <repository>
                <id>rh-ga-repository</id>
                  <url>https://maven.repository.redhat.com/ga</url>
                  <releases>
                    <enabled>true</enabled>
                  </releases>
                  <snapshots>
                    <enabled>false</enabled>
                  </snapshots>
              </repository>
              <repository>
                <id>mvn-central</id>
                  <url>https://repo1.maven.org/maven2</url>
                  <releases>
                    <enabled>true</enabled>
                  </releases>
                  <snapshots>
                    <enabled>false</enabled>
                  </snapshots>
              </repository>
              <repository>
                <id>rh-ea-repository</id>
                  <url>https://maven.repository.redhat.com/earlyaccess/all</url>
                  <releases>
                    <enabled>true</enabled>
                  </releases>
                  <snapshots>
                    <enabled>false</enabled>
                  </snapshots>
              </repository>
            </repositories>
            <pluginRepositories>
              <pluginRepository>
                <id>rh-ga-plugin-repository</id>
                <url>https://maven.repository.redhat.com/ga</url>
                <releases>
                  <enabled>true</enabled>
                </releases>
                <snapshots>
                  <enabled>false</enabled>
                </snapshots>
              </pluginRepository>
              <pluginRepository>
                <id>rh-ea-plugin-repository</id>
                <url>https://maven.repository.redhat.com/earlyaccess/all</url>
                <releases>
                  <enabled>true</enabled>
                </releases>
                <snapshots>
                  <enabled>false</enabled>
                </snapshots>
              </pluginRepository>
              <pluginRepository>
                <id>mvn-central-plugin</id>
                <url>https://repo1.maven.org/maven2</url>
                <releases>
                  <enabled>true</enabled>
                </releases>
                <snapshots>
                  <enabled>false</enabled>
                </snapshots>
              </pluginRepository>
            </pluginRepositories>
          </profile> 
        </profiles>
      <activeProfiles>
        <activeProfile>fuse-profile</activeProfile>
      </activeProfiles>
    </settings>

